{
  "version": 2,
  "builds": [
{ "src": "src/index.js", "use": "@vercel/node" }
],

  "routes": [
    { "src": "/api/(.*)", "dest": "src/index.js" },
    { "src": "/(.*)", "dest": "src/index.js" }
  ],
    "rewrites": [
    { "source": "/api/:path*", "destination": "https://astromania-web-nine.vercel.app/api/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
